  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch Starter",
      "request": "launch",
      "mainClass": "awardcenter.Starter",
      "projectName": "AwardCenter",
      "vmArgs": "-Dengine=awardcenter.engine.JAXBEngine",
      "cwd": "${workspaceFolder}\\target\\classes"
    },
    {
      "type": "java",
      "name": "Launch ColorTableModel",
      "request": "launch",
      "mainClass": "misc.ColorTableModel",
      "projectName": "AwardCenter"
    },
    {
      "type": "java",
      "name": "Launch DiagonalGradientPanel",
      "request": "launch",
      "mainClass": "misc.DiagonalGradientPanel",
      "projectName": "AwardCenter"
    },
    {
      "type": "java",
      "name": "Launch FileCache",
      "request": "launch",
      "mainClass": "misc.FileCache",
      "projectName": "AwardCenter"
    },
    {
      "type": "java",
      "name": "Launch GradPaintAnimation",
      "request": "launch",
      "mainClass": "misc.GradPaintAnimation",
      "projectName": "AwardCenter"
    },
    {
      "type": "java",
      "name": "Launch MakeBig",
      "request": "launch",
      "mainClass": "misc.MakeBig",
      "projectName": "AwardCenter"
    },
    {
      "type": "java",
      "name": "Launch SwingWorkerDemo",
      "request": "launch",
      "mainClass": "misc.SwingWorkerDemo",
      "projectName": "AwardCenter"
    }
  ]
}